@prefix : <workspace:src/rdf/tests/cases/valid-rdfs-subclass-shape.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

:TestCase a owl:Ontology ;
    rdfs:label "SHACL Subclass Test Case"@en .

:shapeA a sh:NodeShape ;
    rdfs:label "Shape A"@en .

:shapeB a sh:NodeShape ;
    rdfs:label "Shape B"@en .

:shapeB rdfs:subClassOf :shapeA .