  skos:scopeNote "Monochrome, mostly brown coloured paper with many fine, mostly black, sprinkles."@en, "brunfarget  papir dekorert med mange små mørke prikker "@nb ;
  skos:broader lobc:3988 ;
  skos:inScheme lob: .

lobc:4900
